<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="ProgressFit.Mobile.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:ProgressFit.Mobile"
    xmlns:workout="clr-namespace:ProgressFit.Mobile.Views.Modules.Workout.Main"
    Shell.FlyoutBehavior="Flyout">


        <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
        
        <ShellContent Title="Workout" ContentTemplate="{DataTemplate workout:WorkoutMainPage}" Route="WorkoutMain" />
  

</Shell>
